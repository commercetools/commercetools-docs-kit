{
  "name": "commercetools-docs-kit",
  "version": "0.0.0",
  "description": "commercetools Documentation Kit (monorepo)",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "websites/*"
  ],
  "scripts": {
    "start": "yarn --cwd websites/docs-smoke-test start",
    "build": "cross-env NODE_ENV=production lerna run build --scope '@commercetools-website/*'",
    "prebuild": "cross-env NODE_ENV=production lerna run build --scope '@commercetools-docs/*'",
    "postbuild": "npx commercetools-broken-link-checker ./public docs-smoke-test/",
    "lint": "jest --projects jest.eslint.config.js jest.stylelint.config.js jest.text.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:css": "jest --config jest.stylelint.config.js",
    "lint:text": "jest --config jest.text.config.js",
    "format": "yarn format:js && yarn format:md && yarn format:yaml",
    "format:js": "prettier --write '**/*.{js,ts,tsx}'",
    "format:md": "prettier --write --parser markdown '**/*.{md,mdx}'",
    "format:yaml": "prettier --write --parser yaml '**/*.yaml'",
    "labels:sync": "github-labels sync",
    "test": "jest --config jest.test.config.js",
    "test:watch": "yarn test --watch",
    "test:e2e": "cross-env NODE_ENV=test cypress run",
    "changelog": "node scripts/changelog.js",
    "check-npm-login": "npm_config_registry=https://registry.npmjs.org npm whoami",
    "prerelease": "yarn check-npm-login && yarn prebuild",
    "release": "lerna publish --exact",
    "release:canary": "lerna publish --exact --dist-tag canary --canary --preid canary --yes"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-do-expressions": "7.7.4",
    "@babel/plugin-proposal-export-default-from": "7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-transform-classes": "7.7.4",
    "@babel/plugin-transform-destructuring": "7.7.4",
    "@babel/plugin-transform-react-display-name": "7.7.4",
    "@babel/plugin-transform-regenerator": "7.7.5",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "@babel/runtime-corejs3": "7.7.6",
    "@commercetools/github-labels": "1.1.0",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@emotion/babel-preset-css-prop": "10.0.23",
    "@emotion/core": "10.0.22",
    "@emotion/styled": "10.0.23",
    "@percy/cypress": "2.2.0",
    "@rollup/plugin-json": "4.0.0",
    "@rollup/plugin-node-resolve": "6.0.0",
    "@svgr/rollup": "4.3.3",
    "@testing-library/cypress": "5.0.2",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.4.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "core-js": "3.5.0",
    "cross-env": "6.0.3",
    "cypress": "3.8.0",
    "dotenv": "8.2.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-cypress": "2.8.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-testing-library": "1.3.4",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-preset-gatsby": "1.0.22",
    "jest-runner-eslint": "0.7.5",
    "jest-runner-executor": "1.0.0",
    "jest-runner-prettier": "0.3.4",
    "jest-runner-stylelint": "2.2.2",
    "jest-silent-reporter": "0.1.2",
    "jest-watch-master": "1.0.0",
    "jest-watch-typeahead": "0.4.2",
    "lerna": "3.19.0",
    "lerna-changelog": "0.8.3",
    "lint-staged": "9.5.0",
    "percy": "0.20.5",
    "prettier": "1.19.1",
    "read-pkg-up": "7.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rollup": "1.27.13",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "serve": "11.2.0",
    "shelljs": "0.8.3",
    "stylelint": "12.0.0",
    "stylelint-config-prettier": "8.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "3.1.1",
    "wait-on": "3.3.0"
  },
  "resolutions": {
    "**/cypress": "3.8.0",
    "**/sharp": "0.23.3"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  }
}
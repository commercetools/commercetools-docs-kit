{
  "name": "commercetools-docs-kit",
  "version": "0.0.0",
  "description": "commercetools Documentation Kit (monorepo)",
  "license": "MIT",
  "private": true,
  "workspaces": ["packages/*", "websites/*"],
  "preconstruct": {
    "packages": ["packages/ui-kit"]
  },
  "scripts": {
    "postinstall": "preconstruct dev",
    "clean": "lerna run clean --scope '@commercetools-website/*'",
    "start": "yarn --cwd websites/docs-smoke-test start",
    "build-packages": "preconstruct build",
    "build": "cross-env NODE_ENV=production lerna run build --scope '@commercetools-website/*'",
    "prebuild": "yarn build-packages && yarn generate-icons",
    "postbuild": "cp -f ./websites/index.html ./public && yarn check-links",
    "generate-icons": "lerna run generate-icons && prettier --write '**/generated/*.js'",
    "lint": "jest --projects jest.eslint.config.js jest.stylelint.config.js jest.text.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:css": "jest --config jest.stylelint.config.js",
    "lint:text": "jest --config jest.text.config.js",
    "format": "yarn format:js && yarn format:md && yarn format:yaml",
    "format:js": "prettier --write '**/*.{js,ts,tsx}'",
    "format:md": "prettier --write --parser markdown '**/*.md'",
    "format:yaml": "prettier --write --parser yaml '**/*.yaml'",
    "labels:sync": "github-labels sync",
    "test": "jest --config jest.test.config.js",
    "test:watch": "yarn test --watch",
    "test:e2e": "yarn test:e2e:docs-smoke-test && yarn test:e2e:api-docs-smoke-test",
    "test:e2e:docs-smoke-test": "cross-env NODE_ENV=test cypress run --spec cypress/integration/docs-smoke-test/**/*",
    "test:e2e:api-docs-smoke-test": "cross-env NODE_ENV=test cypress run --spec cypress/integration/api-docs-smoke-test/**/*",
    "cypress-open": "cross-env NODE_ENV=test cypress open",
    "check-links": "npx linkinator ./public",
    "check-npm-login": "npm_config_registry=https://registry.npmjs.org npm whoami",
    "changeset": "changeset",
    "changeset:version-and-format": "changeset version && prettier --write --parser json '**/package.json'",
    "versions:uikit": "bulk-update-versions --match '@commercetools-uikit/(.*)'"
  },
  "dependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-do-expressions": "7.12.1",
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-classes": "7.12.1",
    "@babel/plugin-transform-destructuring": "7.12.1",
    "@babel/plugin-transform-react-display-name": "7.12.1",
    "@babel/plugin-transform-regenerator": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@babel/runtime": "7.12.5",
    "@babel/runtime-corejs3": "7.12.5",
    "@changesets/changelog-github": "0.2.7",
    "@changesets/cli": "2.12.0",
    "@commercetools/github-labels": "1.1.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@emotion/babel-plugin": "11.0.0",
    "@emotion/babel-preset-css-prop": "11.0.0",
    "@percy/cypress": "2.3.3",
    "@preconstruct/cli": "2.0.0",
    "@svgr/cli": "5.5.0",
    "@svgr/plugin-jsx": "5.5.0",
    "@svgr/plugin-svgo": "5.5.0",
    "@testing-library/cypress": "7.0.2",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "bulk-update-versions": "1.1.1",
    "core-js": "3.8.0",
    "cross-env": "7.0.3",
    "cypress": "5.6.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-formatter-pretty": "4.0.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.1",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-preset-gatsby": "1.0.100",
    "jest-runner-eslint": "0.10.0",
    "jest-runner-executor": "1.0.0",
    "jest-runner-prettier": "0.3.6",
    "jest-runner-stylelint": "2.3.7",
    "jest-silent-reporter": "0.4.0",
    "jest-watch-master": "1.0.0",
    "jest-watch-typeahead": "0.6.1",
    "lerna": "3.22.1",
    "linkinator": "2.5.1",
    "lint-staged": "10.5.2",
    "percy": "0.28.5",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "read-pkg-up": "7.0.1",
    "serve": "11.3.2",
    "shelljs": "0.8.4",
    "start-server-and-test": "1.11.6",
    "stylelint": "13.8.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "wait-on": "5.2.0"
  },
  "resolutions": {
    "**/cypress": "5.6.0",
    "**/sharp": "0.26.3",
    "**/gatsby-link": "2.7.0"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=5"
  }
}

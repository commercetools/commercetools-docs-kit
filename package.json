{
  "name": "commercetools-docs-kit",
  "version": "0.0.0",
  "description": "commercetools Documentation Kit (monorepo)",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "websites/*"
  ],
  "scripts": {
    "start": "yarn --cwd websites/docs-smoke-test start",
    "build": "NODE_ENV=production lerna run build --scope '@commercetools-local/*'",
    "lint": "jest --projects jest.eslint.config.js jest.stylelint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format": "yarn format:js && yarn format:css && yarn format:md && yarn format:yaml",
    "format:js": "prettier --write '**/*.{js,ts,tsx}'",
    "format:md": "prettier --write --parser markdown '**/*.{md,mdx}'",
    "format:yaml": "prettier --write --parser yaml '**/*.yaml'",
    "labels:sync": "github-labels sync",
    "test": "jest --config jest.test.config.js",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "@babel/core": "7.7.0",
    "@commercetools/github-labels": "1.1.0",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@emotion/core": "10.0.22",
    "@emotion/styled": "10.0.23",
    "@testing-library/jest-dom": "4.2.3",
    "@testing-library/react": "9.3.2",
    "babel-eslint": "10.0.3",
    "core-js": "3.3.6",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-formatter-pretty": "2.1.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "eslint-plugin-testing-library": "1.2.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "jest-preset-gatsby": "1.0.13",
    "jest-runner-eslint": "0.7.5",
    "jest-runner-prettier": "0.3.1",
    "jest-runner-stylelint": "2.2.0",
    "jest-silent-reporter": "0.1.2",
    "jest-watch-master": "1.0.0",
    "jest-watch-typeahead": "0.4.0",
    "lerna": "3.18.3",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "stylelint": "11.1.1",
    "stylelint-config-prettier": "6.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "3.1.1"
  },
  "resolutions": {
    "**/sharp": "0.22.1"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  }
}
{
  "name": "commercetools-docs-kit",
  "version": "0.0.0",
  "description": "commercetools Documentation Kit (monorepo)",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "websites/*"
  ],
  "scripts": {
    "start": "yarn --cwd websites/docs-smoke-test start",
    "build": "cross-env NODE_ENV=production lerna run build --scope '@commercetools-website/*'",
    "prebuild": "cross-env NODE_ENV=production lerna run build --scope '@commercetools-docs/*'",
    "postbuild": "npx commercetools-broken-link-checker ./public docs-smoke-test/",
    "lint": "jest --projects jest.eslint.config.js jest.stylelint.config.js jest.text.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:css": "jest --config jest.stylelint.config.js",
    "lint:text": "jest --config jest.text.config.js",
    "format": "yarn format:js && yarn format:md && yarn format:yaml",
    "format:js": "prettier --write '**/*.{js,ts,tsx}'",
    "format:md": "prettier --write --parser markdown '**/*.md'",
    "format:yaml": "prettier --write --parser yaml '**/*.yaml'",
    "labels:sync": "github-labels sync",
    "test": "jest --config jest.test.config.js",
    "test:watch": "yarn test --watch",
    "test:e2e": "cross-env NODE_ENV=test cypress run",
    "cypress-open": "cross-env NODE_ENV=test cypress open",
    "changelog": "node scripts/changelog.js",
    "check-npm-login": "npm_config_registry=https://registry.npmjs.org npm whoami",
    "prerelease": "yarn check-npm-login && yarn prebuild",
    "prerelease:canary": "yarn prerelease",
    "release": "lerna publish --exact",
    "release:canary": "lerna publish --exact --dist-tag canary --canary --preid canary --yes --force-publish \"*\""
  },
  "devDependencies": {
    "@actions/core": "1.2.3",
    "@actions/github": "2.1.1",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-do-expressions": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-classes": "7.9.2",
    "@babel/plugin-transform-destructuring": "7.8.8",
    "@babel/plugin-transform-react-display-name": "7.8.3",
    "@babel/plugin-transform-regenerator": "7.8.7",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.1",
    "@babel/runtime-corejs3": "7.9.2",
    "@commercetools/github-labels": "1.1.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "@percy/cypress": "2.3.0",
    "@rollup/plugin-json": "4.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@svgr/rollup": "5.1.0",
    "@testing-library/cypress": "6.0.0",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-transform-dynamic-import": "2.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "core-js": "3.6.4",
    "cross-env": "7.0.0",
    "cypress": "4.0.2",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-cypress": "2.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "3.0.0",
    "eslint-plugin-testing-library": "3.0.2",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jest-preset-gatsby": "1.0.29",
    "jest-runner-eslint": "0.7.6",
    "jest-runner-executor": "1.0.0",
    "jest-runner-prettier": "0.3.6",
    "jest-runner-stylelint": "2.3.1",
    "jest-silent-reporter": "0.2.1",
    "jest-watch-master": "1.0.0",
    "jest-watch-typeahead": "0.4.2",
    "lerna": "3.20.2",
    "lerna-changelog": "1.0.0",
    "lint-staged": "10.0.7",
    "percy": "0.21.0",
    "prettier": "2.0.1",
    "read-pkg-up": "7.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rollup": "2.0.6",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-peer-deps-external": "2.2.2",
    "serve": "11.3.0",
    "shelljs": "0.8.3",
    "stylelint": "13.2.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "wait-on": "4.0.0"
  },
  "resolutions": {
    "**/cypress": "4.0.2",
    "**/sharp": "0.23.3"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  }
}
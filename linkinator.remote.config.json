{
  "__available_flags": "https://github.com/JustinBeckwith/linkinator/blob/main/src/config.ts#L3",
  "recurse": true,
  "skip": [
    "/.+/(page-data|static|icons)/(.*)",
    "/[^/]+/[^/]+.(js|json)$",
    "/.+/favicon(.*)",
    "https://support.commercetools.com",
    "/.+/api$",
    "/.+/api/",
    "/.+/custom-applications",
    "/.+/docs/",
    "/.+/import-export",
    "/.+/merchant-center",
    "/.+/sdk",
    "/.+/tutorials",
    "/.+/getting-started"
  ],
  "verbosity": "error",
  "retry": true,
  "retryErrors": true,
  "retryErrorsCount": 3,
  "retryErrorsJitter": 500,
  "urlRewriteSearch": "https://docs.commercetools.com"
}

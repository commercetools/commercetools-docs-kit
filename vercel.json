{
  "version": 2,
  "public": true,
  "name": "commercetools-docs-kit",
  "alias": "commercetools-docs-kit",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/", "destination": "/docs-smoke-test/", "statusCode": 302 },
    { "source": "/(\\S+?)/.*", "destination": "/$1/404", "statusCode": 404 }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/docs-smoke-test/404" }]
}

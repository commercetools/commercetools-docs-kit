{
  "version": 2,
  "public": true,
  "name": "commercetools-docs-kit",
  "alias": "commercetools-docs-kit",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(\\S+?)/.*",
      "status": 404,
      "dest": "/$1/4,04"
    }
  ],
  "redirects": [
    { "source": "/", "destination": "/docs-smoke-test/", "statusCode": 302 },
    {
      "source": "/(.*)",
      "destination": "/docs-smoke-test/404",
      "statusCode": 404
    }
  ]
}

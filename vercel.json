{
  "version": 2,
  "public": true,
  "name": "commercetools-docs-kit",
  "alias": "commercetools-docs-kit",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/", "destination": "/docs-smoke-test/", "statusCode": 302 }
  ],
  "rewrites": [
    {
      "source": "/(docs-smoke-test|api-docs-smoke-test)/(.*)",
      "destination": "/$1/404"
    },
    { "source": "/(.*)", "destination": "/docs-smoke-test/404" }
  ]
}

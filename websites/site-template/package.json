{
  "name": "@commercetools-website/site-template",
  "version": "3.1.1",
  "private": true,
  "scripts": {
    "start": "yarn develop",
    "develop": "npx gatsby develop",
    "serve": "npx gatsby serve --prefix-paths -o",
    "clean": "npx gatsby clean",
    "clean:develop": "yarn clean && yarn develop",
    "build": "npx gatsby build --prefix-paths",
    "prebuild": "yarn clean",
    "postbuild": "rm -rf ../../public/site-template && mkdir -p ../../public && mv public ../../public/site-template"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "11.0.1",
    "gatsby": "2.27.3",
    "gatsby-cli": "2.14.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}

{
  "name": "@commercetools-website/site-template",
  "version": "4.0.1",
  "private": true,
  "scripts": {
    "start": "yarn develop",
    "develop": "npx gatsby develop",
    "serve": "npx gatsby serve --prefix-paths -o",
    "clean": "npx gatsby clean",
    "clean:develop": "yarn clean && yarn develop",
    "build": "npx gatsby build --prefix-paths",
    "prebuild": "mkdir -p ../../public/site-template && rm -rf public && mv ../../public/site-template ./public && mkdir -p ../../.cache/site-template && rm -rf .cache && mv ../../.cache/site-template ./.cache",
    "postbuild": "mv public ../../public/site-template && mv .cache ../../.cache/site-template"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-docs": "15.0.1",
    "gatsby": "3.0.4",
    "gatsby-cli": "3.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}

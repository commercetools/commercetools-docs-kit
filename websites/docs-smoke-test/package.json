    "build:serve": "npx gatsby build --prefix-paths && npx gatsby serve --prefix-paths -o",
    "prebuild": "yarn clean",
    "postbuild": "rm -rf ../../public/docs-smoke-test && mkdir -p ../../public && mv public ../../public/docs-smoke-test"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-code-examples": "3.0.2",
    "@commercetools-docs/gatsby-theme-constants": "3.0.2",
    "@commercetools-docs/gatsby-theme-docs": "4.0.1",
    "@commercetools-uikit/card": "10.18.4",
    "gatsby": "2.21.39",
    "gatsby-cli": "2.12.24"
  }
}

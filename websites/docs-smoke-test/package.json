{
  "name": "@commercetools-website/docs-smoke-test",
  "version": "7.1.1",
  "private": true,
  "browserslist": [">0.25%", "not dead and supports es6-module"],
  "scripts": {
    "start": "yarn develop",
    "develop": "yarn gatsby develop",
    "serve": "yarn gatsby serve --prefix-paths -o",
    "clean": "yarn gatsby clean",
    "clean:develop": "yarn clean && yarn develop",
    "create-docs-release-note": "yarn create-docs-release-note",
    "build": "./scripts/build.sh",
    "build:serve": "yarn gatsby build --prefix-paths && yarn gatsby serve --prefix-paths -o",
    "build:analyze": "ANALYZE_BUNDLE=true yarn build",
    "generate-icons": "svgr -d src/icons/generated -- src/icons/svg"
  },
  "dependencies": {
    "@commercetools-docs/gatsby-theme-code-examples": "21.1.0",
    "@commercetools-docs/gatsby-theme-constants": "21.1.0",
    "@commercetools-docs/gatsby-theme-docs": "21.1.1",
    "@commercetools-docs/gatsby-theme-learning": "21.1.1",
    "@commercetools-docs/gatsby-theme-sso-ui-kit": "21.1.1",
    "@commercetools-docs/ui-kit": "21.1.0",
    "@commercetools-uikit/avatar": "15.3.0",
    "@commercetools-uikit/icons": "15.3.0",
    "@commercetools-uikit/radio-input": "15.3.0",
    "gatsby": "5.7.0",
    "gatsby-cli": "5.7.0",
    "process-top": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@svgr/cli": "6.5.1"
  }
}

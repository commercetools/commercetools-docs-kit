#%RAML 1.0 DataType
displayName: NumberSetAttribute
type: Attribute
(builtinType): object
discriminatorValue: number-set
description: |-
  This type represents an attribute which value is a set of numbers.
properties:
  type:
    type: string
    (builtinType): string
    enum:
    - number-set
    required: true
  value:
    type: array
    items:
      type: number
    (builtinType): array
    required: true